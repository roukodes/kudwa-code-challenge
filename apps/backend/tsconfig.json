{
  // "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "lib": ["ESNext"],
    "types": ["node", "vitest/globals"],
    "baseUrl": "./",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "paths": {
      "@/*": ["src/*"],
      "@data/*": ["data/*"]
    },
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "removeComments": false
  }
}
